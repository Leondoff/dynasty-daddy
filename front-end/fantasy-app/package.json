    "chartjs-plugin-datalabels": "^1.0.0",
    "d3": "^7.8.5",
    "express": "^4.17.1",
    "grosso-modo": "^4.3.0",
    "http-server": "^14.1.1",
    "javascript-color-gradient": "^2.4.4",
